<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.Button?>


<StackPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:id="root" fx:controller="com.deadlyspeed.controllers.LobbyController" stylesheets="/styles/lobby.css" styleClass="root">
    <children>
      <VBox prefHeight="200.0" prefWidth="100.0" spacing="20.0" style="-fx-background-color: #000000;">
         <children>
           <Text text="Deadly Speed" />
           <Button onAction="#quickLogIn" text="Quick Game" />
           <Button onAction="#getLeagueLobby" text="Champions League" />
           <Button onAction="#quitGame" text="Quit" />
         </children>
      </VBox>
    </children>
</StackPane>
